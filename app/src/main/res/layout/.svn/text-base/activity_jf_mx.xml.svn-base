<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.ayj.chunmiao.activity.kc.jf.JfMxActivity">
    <include layout="@layout/common_title_layout"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/dark_gray"
        android:gravity="center_vertical">
        <TextView
            android:id="@+id/tv_title_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/edit_text"
            android:textSize="18dp"
            android:gravity="center"
            android:text="日期"/>
        <TextView
            android:id="@+id/tv_title_snid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/edit_text"
            android:textSize="18dp"
            android:gravity="center"
            android:text="流水号"/>
        <TextView
            android:id="@+id/tv_title_num"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/edit_text"
            android:textSize="18dp"
            android:gravity="center"/>

    </LinearLayout>
    <cn.finalteam.loadingviewfinal.PtrClassicFrameLayout
        android:id="@+id/pcf_refresh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <cn.finalteam.loadingviewfinal.RecyclerViewFinal
            android:id="@+id/recycler_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </cn.finalteam.loadingviewfinal.RecyclerViewFinal>
    </cn.finalteam.loadingviewfinal.PtrClassicFrameLayout>
</LinearLayout>
