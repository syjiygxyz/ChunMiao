                    android:layout_weight="1"
                    android:gravity="center_horizontal">


                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/main_center_4" />

                    <TextView
                        android:id="@+id/tv_qd_4"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginLeft="25dp"
                        android:background="@drawable/red_corner"
                        android:gravity="center"
                        android:text="11"
                        android:textColor="@color/white"
                        android:textSize="10dp"
                        android:visibility="gone" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_qd_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_horizontal">


                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/main_center_5" />

                    <TextView
                        android:id="@+id/tv_qd_5"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginLeft="25dp"
                        android:background="@drawable/red_corner"
                        android:gravity="center"
                        android:text="11"
                        android:textColor="@color/white"
                        android:textSize="10dp"
                        android:visibility="gone" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_qd_6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_horizontal">


                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/main_center_6" />

                    <TextView
                        android:id="@+id/tv_qd_6"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginLeft="25dp"
                        android:background="@drawable/red_corner"
                        android:gravity="center"
                        android:text="11"
                        android:textColor="@color/white"
                        android:textSize="10dp"
                        android:visibility="gone" />
                </RelativeLayout>
            </LinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/rlv_bottom"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:layout_weight="1" />
        </LinearLayout>
    </cn.finalteam.loadingviewfinal.PtrClassicFrameLayout>
</LinearLayout>